{
	"name": "ts-prisma",
	"version": "1.0.0",
	"description": "",
	"main": "index.js",
	"prisma": {
		"seed": "npx ts-node prisma/seed.ts"
	},
	"scripts": {
		"dev open": "npx nuxi dev -o --host",
		"build & preview": "npx nuxi build && npx nuxi preview",
		"cleanup": "npx nuxi cleanup",
		"prisma delete": "rd /s /q prisma\\migrations && del /f /q /a prisma\\db.sqlite*",
		"prisma migrate": "npx prisma migrate dev --name init",
		"prisma generate": "npx prisma generate",
		"prisma db pull": "npx prisma db pull",
		"prisma db push": "npx prisma db push",
		"prisma seed": "npx prisma db seed",
		"prisma studio": "npx prisma studio",
		"cleanup & delete": "npx nuxi cleanup && rd /s /q node_modules",
		"ts:insertUser": "npx ts-node insertUser.ts",
		"ts:insertUserPost": "npx ts-node insertUserPost.ts",
		"ts:insertUserProfile": "npx ts-node insertUserProfile.ts",
		"ts:insertUserProfilePost": "npx ts-node insertUserProfilePost.ts",
		"ts:insertProfile": "npx ts-node insertProfile.ts",
		"ts:selectUsers": "npx ts-node selectUsers.ts",
		"ts:selectUsersPost": "npx ts-node selectUsersPost.ts",
		"ts:selectUsersProfilePost": "npx ts-node selectUsersProfilePost.ts",
		"ts:updatePost": "npx ts-node updatePost.ts"
	},
	"keywords": [],
	"author": "",
	"license": "ISC",
	"devDependencies": {
		"@types/node": "^20.11.5",
		"prisma": "^5.8.1"
	},
	"dependencies": {
		"@prisma/client": "^5.8.1"
	}
}
